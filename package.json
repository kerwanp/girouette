{
  "name": "@adonisjs-community/girouette",
  "description": "An AdonisJS package allowing decorators-based routing.",
  "version": "0.1.5",
  "engines": {
    "node": ">=20.6.0"
  },
  "type": "module",
  "files": [
    "build/src",
    "build/providers",
    "build/stubs",
    "build/index.d.ts",
    "build/index.js",
    "build/configure.d.ts",
    "build/configure.js"
  ],
  "exports": {
    ".": "./build/index.js",
    "./girouette_provider": "./build/providers/girouette_provider.js"
  },
  "scripts": {
    "clean": "del-cli build",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --ext=.ts",
    "format": "prettier --write .",
    "quick:test": "node --import=./tsnode.esm.js --enable-source-maps bin/test.ts",
    "pretest": "npm run lint",
    "copy:templates": "copyfiles --up 1 \"stubs/**/*.stub\" build/stubs",
    "test": "c8 --include=src --include=providers --reporter=json-summary npm run quick:test && istanbul-badges-readme",
    "prebuild": "npm run lint && npm run clean",
    "build": "pnpm clean && tsc && pnpm copy:templates",
    "release": "release-it",
    "version": "npm run build",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "@adonisjs/assembler": "^7.7.0",
    "@adonisjs/core": "^6.18.0",
    "@adonisjs/eslint-config": "^1.3.0",
    "@adonisjs/prettier-config": "^1.3.0",
    "@adonisjs/tsconfig": "^1.3.0",
    "@japa/assert": "^3.0.0",
    "@japa/runner": "^3.1.4",
    "@swc/core": "^1.12.5",
    "@types/node": "^20.14.5",
    "c8": "^10.1.2",
    "copyfiles": "^2.4.1",
    "del-cli": "^5.1.0",
    "eslint": "^8.57.0",
    "istanbul-badges-readme": "^1.9.0",
    "np": "^10.0.6",
    "prettier": "^3.3.2",
    "release-it": "^19.0.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "reflect-metadata": "^0.2.2"
  },
  "peerDependencies": {
    "@adonisjs/core": "^6.2.0"
  },
  "author": "Alexis Bouchez <alexis@softwarecitadel.com>",
  "license": "MIT",
  "keywords": [
    "AdonisJS",
    "decorators",
    "routing"
  ],
  "eslintConfig": {
    "extends": "@adonisjs/eslint-config/package"
  },
  "prettier": "@adonisjs/prettier-config",
  "publishConfig": {
    "access": "public",
    "tag": "latest"
  },
  "repository": "github:adonisjs-community/girouette",
  "c8": {
    "reporter": [
      "text",
      "html"
    ],
    "exclude": [
      "tests/**"
    ]
  }
}
